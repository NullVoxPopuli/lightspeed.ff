{
  "manifest_version": 2,
  "name": "lightspeed.ff",
  "version": "0.0.1",
  "description": "lightspeed.nvim, but for the browser",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "applications": {
    "gecko": {
      "id": "nvp-permissionless@lightspeed.ff"
    }
  },
  "background": {
    "scripts": ["background_scripts/index.js"]
  },
  "options_ui": {
    "page": "pages/options.html",
    "chrome_style": false,
    "open_in_tab": true
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_scripts/index.js"],
      "css": ["content_scripts/vimium.css"],
      "run_at": "document_end",
      "all_frames": true,
      "match_about_blank": true
    }
  ],
  "browser_action": {
    "default_icon": "icons/browser_action_disabled.png",
    "default_popup": "pages/popup.html"
  },
  "web_accessible_resources": [
    "content_scripts/vimium.css",
    "pages/hud.html",
    "pages/help_dialog.html"
  ]
}
